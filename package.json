{
  "name": "nextjs-builder-io-landing-page",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "find:unused": "next-unused",
    "prettier": "prettier"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@builderComponents/*": [
        "builderComponents/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages",
      "sections"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  },
  "dependencies": {
    "@builder.io/react": "^1.1.47",
    "@vercel/fetch": "^6.1.1",
    "atob": "^2.1.2",
    "jest": "^26.6.3",
    "next": "^12.0.4",
    "next-seo": "^4.28.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-merge-refs": "^1.1.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.0.4",
    "@types/node": "^16.11.11",
    "@types/react": "^17.0.37",
    "autoprefixer": "^10.4.0",
    "bunyan": "^1.8.15",
    "bunyan-prettystream": "^0.1.3",
    "next-unused": "^0.0.6",
    "postcss": "^8.4.4",
    "prettier": "^2.5.0",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.5.2"
  },
  "resolutions": {
    "webpack": "^5.0.0-beta.30"
  },
  "license": "MIT"
}
